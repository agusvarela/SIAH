<body class="landing-page">
    <nav class="navbar navbar-transparent navbar-absolute" role="navigation">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <div class="logo-container">
                    <div class="logo">
                        <img src="~/Content/img/logo.jpg" alt="AH Logo">
                    </div>

                    @Html.ActionLink(" Autorizaciones Hospitalarias", "Index", "Home", new { area = "" }, new { @class = " logo-container brand" })

                </div>
            </div>
         
       
            <div class="collapse navbar-collapse" id="navigation-index">
                <ul class="nav navbar-nav navbar-right">
                    <li>
                        @if (Session["userid"] != null)
                        {
                            <a>
                                Ha iniciado sesion como @Session["nombre"].ToString()
                            </a>
                                @Html.ActionLink("(Cerrar Sesion)", "Logout", "Account" );
                           
                        }
                    </li>
                    <li>@Html.ActionLink("Listado De Pedidos", "Listado", "Pedidos")</li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Administrar<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li><a href="../Insumos/Index">Insumo</a></li>
