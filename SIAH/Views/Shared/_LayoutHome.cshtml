                             <li class="divider"></li>
                              <li><a href="#">One more separated link</a></li>*@
                        </ul>
                    </li>
                </ul>
            </div>
        </div>

    </nav>      @RenderBody()
        <hr />
    <footer class="footer">
        <div class="container">
            
            <div class="copyright pull-right">
