
<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <link rel="shortcut icon" type="image/x-icon" href="~/favicon.ico" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>SIAH - Autorizaciones Hospitalarias</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <!--     Fonts and icons     -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons" />
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />
    <script src="https://code.jquery.com/jquery-3.2.1.js" integrity="sha256-DZAnKJ/6XZ9si04Hgrsxu/8s717jcIzLy3oi35EouyE=" crossorigin="anonymous"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css" />
    @*@Scripts.Render("https://code.jquery.com/jquery-3.2.1.js")*@
    @*@Scripts.Render("https://code.jquery.com/ui/1.12.1/jquery-ui.js")*@
    @*@Scripts.Render("https://code.jquery.com/jquery-3.2.1.min.js")*@
    @*@Scripts.Render("~/bundles/bootstrap")*@
    @*@Styles.Render("~/Content/themes/base/css")*@
    @*@Scripts.Render("~/bundles/jquery")*@
    <!--     Fonts and icons     -->
    <script src="https://code.jquery.com/jquery-3.2.1.js" integrity="sha256-DZAnKJ/6XZ9si04Hgrsxu/8s717jcIzLy3oi35EouyE=" crossorigin="anonymous"></script>
    <script src="../../Scripts/jquery-1.12.4.js" type="text/javascript"></script>
    <script src="../../Scripts/jquery-1.12.4.min.js" type="text/javascript"></script>
    <script src="../../Scripts/jquery-ui-1.12.1.js" type="text/javascript"></script>
    @Scripts.Render("~/bundles/jqueryui")
    @Styles.Render("~/Content/app.css")
    @RenderSection("scripts", required: false)
    <!-- CSS Files -->
    <link href="~/Content/css/animate.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/css/bootstrap.min.css")
    @Styles.Render("~/Content/css/SIAHStyle.css")
    @Styles.Render("~/Content/css/demo.css")
    <script src="~/Content/js/material.min.js"></script>
    <script src="~/Content/js/material-kit.js"></script>
</head>
<body class="signup-page">
    <nav class="navbar navbar-transparent navbar-absolute" role="navigation">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="logo-container" href="@Url.Action("Index", "Home", null)">
                    <div class="logo">
                        <img src="~/Content/img/logo.jpg" alt="AH Logo">
                    </div>
                    @Html.Label(" Autorizaciones Hospitalarias", new { @class = " logo-container brand" })
                </a>
            </div>


            <div class="collapse navbar-collapse" id="navigation-index">
                <ul class="nav navbar-nav navbar-right">
                    <li class="">
                        @if (Session["userid"] != null)
                        {
                            <a>Ha iniciado sesion como @Session["nombre"].ToString()</a>
                        }
                    </li>
                    @*<li>@Html.ActionLink("Nuevo Usuario", "Register", "Account")</li>*@
                    @if (Session["userid"] != null)
                    {
                        <li>@Html.ActionLink("Cerrar Sesion", "Logout", "Account")</li>
                    }
                </ul>
            </div>
        </div>
    </nav>      
    @RenderBody()

</body>
</html>