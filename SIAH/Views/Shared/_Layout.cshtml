<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    <!--     Fonts and icons     -->
    <script src="https://code.jquery.com/jquery-3.2.1.js" integrity="sha256-DZAnKJ/6XZ9si04Hgrsxu/8s717jcIzLy3oi35EouyE=" crossorigin="anonymous"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons" />
    @*<link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700" />*@
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css" />
    @*@Scripts.Render("https://code.jquery.com/jquery-3.2.1.js")*@
    @*@Scripts.Render("https://code.jquery.com/ui/1.12.1/jquery-ui.js")*@
    @*@Scripts.Render("https://code.jquery.com/jquery-3.2.1.min.js")*@
    @*@Scripts.Render("~/bundles/bootstrap")*@
    @*@Styles.Render("~/Content/themes/base/css")*@
    @*@Scripts.Render("~/bundles/jquery")*@
   @Scripts.Render("~/bundles/jqueryui")
    @Styles.Render("~/Content/app.css")
    @RenderSection("scripts", required: false)
    <!-- CSS Files -->
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/css/bootstrap.min.css")
    @Styles.Render("~/Content/css/material-kit.css")
    @Styles.Render("~/Content/css/demo.css")
    

</head>
<body>
    <div class="navbar navbar-default" role="navigation">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <div class="logo-container">
                    <div class="logo">
                        <img src="~/Content/img/logo.png" alt="AH Logo">
                    </div>

                    @Html.ActionLink(" Autorizaciones Hospitalarias", "Index", "Home", new { area = "" }, new { @class = " logo-container brand" })

                </div>
            </div>
            <div class="collapse navbar-collapse" id="navigation-index">
                <ul class="nav navbar-nav navbar-right">
                    <li class="">@if (Session["userid"] != null)
                    {
                        <a>Ha iniciado sesion como @Session["nombre"].ToString()</a>
                    }</li>
                    <li>@Html.ActionLink("Inicio", "Index", "Home")</li>
                    <li>@Html.ActionLink("Listado De Pedidos", "Listado", "Pedidos")</li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Agregar<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li><a href="../Insumos/Index">Insumo</a></li>
                            <li><a href="../Hospitales/Index">Hospital</a></li>
                            <li><a href="../Localidades/Index">Localidad</a></li>
                            <li><a href="../TiposInsumo/Index">Tipo Insumo</a></li>
                            @*<li class="divider"></li>
                            <li><a href="#">Separated link</a></li>
                            <li class="divider"></li>
                            <li><a href="#">One more separated link</a></li>*@
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content" style="background-color: white">
            @RenderBody()
        </div>
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - SIAH ASP.NET</p>
        </footer>
    </div>

    
</body>
</html>
